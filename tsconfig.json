{
	"extends": "astro/tsconfigs/strictest",
	"include": [".astro/types.d.ts", "**/*"],
	"exclude": ["dist"],
	"compilerOptions": {
		"paths": {
			"@components/*": ["./src/components/*"],
			"@layouts/*": ["./src/layouts/*"],
			"@stores/*": ["./src/stores/*"],
			"@effects/*": ["./src/effects/*"],
			"@lib/*": ["./src/lib/*"],
			"@utils/*": ["./src/utils/*"],
			"@images/*": ["./src/images/*"]
		}
	}
}
